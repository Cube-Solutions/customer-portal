{
    "name": "oro/customer-portal",
    "homepage": "https://github.com/oroinc/customer-portal",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocommerce.com"
        }
    ],
    "require": {
        "oro/platform": "2.4.*",
        "oro/platform-serialised-fields": "2.4.*",
        "oro/calendar-bundle": "2.4.*",
        "oro/marketing": "2.4.*",
        "oro/commerce": "1.4.*",
        "leafo/scssphp": "0.6.3",
        "bower-asset/prism": "1.6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"": "src/"},
        "exclude-from-classmap": ["/Tests/"]
    }
}
